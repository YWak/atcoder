package main

import (
	"fmt"
	gomath "math"
	"math/bits"
	"os"
	"sort"
	"strings"

	io "github.com/ywak/atcoder/lib/io"
	"github.com/ywak/atcoder/lib/math"
)

func calc(in *io.In, out *io.Out) {
	n := in.NextInt()
	a := in.NextInts(n)

	const B = 3000
	N := n / B

	sorted := make([][]int, N)
	sum := make([][]int, N)
	for b := 0; b < N; b++ {
		sorted[b] = make([]int, B)
		for i := 0; i < B; i++ {
			sorted[b][i] = a[b*B+i]
		}
		sort.Ints(sorted[b])
		sum[b] = make([]int, B+1)
		for i := 0; i < B; i++ {
			sum[b][i+1] = sum[b][i] + sorted[b][i]
		}
	}

	// [0, r)に含まれるx以下の値の合計
	f := func(r, x int) int {
		s := 0
		for b := 0; b < r/B; b++ {
			ok, ng := -1, B
			for abs(ok-ng) > 1 {
				mid := (ok + ng) / 2
				// debug(r, b, mid, len(sorted), len(sorted[b]))
				if sorted[b][mid] <= x {
					ok = mid
				} else {
					ng = mid
				}
			}
			s += sum[b][ok+1]
		}
		for i := r / B * B; i < r; i++ {
			if a[i] <= x {
				s += a[i]
			}
		}

		return s
	}

	ans := 0
	for q := in.NextInt(); q > 0; q-- {
		u, v, w := in.NextInt3()
		l, r, x := u^ans, v^ans, w^ans

		ans = f(r, x) - f(l-1, x)
		out.Println(ans)
	}
}

func main() {
	in, out := io.InitIo(&io.InitIoProps{
		DoBuffer: true, // インタラクティブならfalseにすること。
		ReadLine: false,
	})
	defer out.Flush()

	calc(in, out)
}

