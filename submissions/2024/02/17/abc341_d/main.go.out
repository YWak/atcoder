func solve2(n, m, k int) int {
	ans := 0
	t := 0
	for c := 1; true; c++ {
		if (c%n == 0) != (c%m == 0) {
			t++
			if t == k {
				ans = c
				break
			}
		}
	}
	return ans
}

func solve1(n, m, k int) int {
	l := lcm(n, m)

	// l以下で条件にあう数はn+m-2個ある
	lc := l/n + l/m - 2

	// kはlc個ずつ飛ばせる。
	b := (k / lc) * l
	k %= lc

	// b以上の正の整数であって、条件を満たす数のうち小さい方からk番目を返す。
	vn, vm := b+n, b+m

	ans := max(b-n, b-m)
	for t := 0; t < k; t++ {
		if vn < vm {
			ans = vn
			vn += n
		} else {
			ans = vm
			vm += m
		}
	}

	return ans
}

func calc(in *io.In, out *io.Out) {
	n, m, k := in.NextInt3()

	out.Println(solve1(n, m, k))
