package main

import (
	"fmt"
	gomath "math"
	"math/bits"
	"os"
	"strings"

	"github.com/ywak/atcoder/lib/ds"
	io "github.com/ywak/atcoder/lib/io"
	"github.com/ywak/atcoder/lib/math"
)

func calc(in *io.In, out *io.Out) {
	n, m := in.NextInt2()
	d := ds.NewInt2d(n, n, INF18)
	canMove := ds.NewBool2d(n, n, false)
	for i := 0; i < m; i++ {
		u, v, w := in.NextInt3()
		u--
		v--
		d[u][v] = w
		canMove[u][v] = true
	}

	// 全点対距離
	for k := 0; k < n; k++ {
		for i := 0; i < n; i++ {
			for j := 0; j < n; j++ {
				if canMove[i][k] && canMove[k][j] && chmin(&d[i][j], d[i][k]+d[k][j]) {
					canMove[i][j] = true
				}
			}
		}
	}

	// TSP
	dp := ds.NewInt2d(1<<n, n, INF18)
	for i := 0; i < n; i++ {
		dp[1<<i][i] = 0
	}
	from := make([]int, 0, n)
	to := make([]int, 0, n)
	for p := 0; p < 1<<n; p++ {
		from = from[0:0]
		to = to[0:0]

		for i := 0; i < n; i++ {
			if nthbit(p, i) == 0 {
				to = append(to, i)
			} else if dp[p][i] != INF18 {
				from = append(from, i)
			}
		}

		// 移動
		for _, x := range from {
			if dp[p][x] == INF18 {
				continue
			}

			for _, y := range to {
				if canMove[x][y] {
					chmin(&dp[p|1<<y][y], dp[p][x]+d[x][y])
				}
			}
		}
	}

	ans := INF18
	for i := 0; i < n; i++ {
		chmin(&ans, dp[1<<n-1][i])
	}
	if ans == INF18 {
		out.Println("No")
	} else {
		out.Println(ans)
	}
}

func main() {
	in, out := io.InitIo(&io.InitIoProps{
		DoBuffer: true, // インタラクティブならfalseにすること。
		ReadLine: false,
	})
	defer out.Flush()

	calc(in, out)
}

