package main

import (
	"fmt"
	gomath "math"
	"math/bits"
	"os"
	"strings"

	"github.com/ywak/atcoder/lib/ds"
	io "github.com/ywak/atcoder/lib/io"
	"github.com/ywak/atcoder/lib/math"
)

func calc(in *io.In, out *io.Out) {
	n, t, m := in.NextInt3()
	ok := ds.NewBool2d(n, n, true)
	for i := 0; i < m; i++ {
		a, b := in.NextInt2()
		ok[a-1][b-1] = false
		ok[b-1][a-1] = false
	}

	// 使用可能なチーム
	can := make([]bool, 1<<n)
	for pat := 0; pat < 1<<n; pat++ {
		can[pat] = true
		for i := 0; i < n; i++ {
			for j := i + 1; j < n; j++ {
				if nthbit(pat, i) == 1 && nthbit(pat, j) == 1 {
					can[pat] = can[pat] && ok[i][j]
				}
			}
		}
	}

	// patの人物が使用済みで、kチーム作成済みのときの場合の数
	full := ((1 << n) - 1)
	var dfs func(pat, k int) int
	dfs = func(p1, k int) int {
		// 作成完了
		if k == t {
			// 全員使っているならok
			if p1 == full {
				return 1
			}
			return 0
		}

		// 最小の人物を決める
		mi := n + 1
		for i := 0; i < n; i++ {
			if nthbit(p1, i) == 0 {
				chmin(&mi, i)
			}
		}

		c := 0
		for p2 := 1; p2 < 1<<n; p2++ {
			if nthbit(p2, mi) == 0 {
				continue
			}
			if !can[p2] {
				continue
			}
			if p1&p2 != 0 {
				continue
			}

			c += dfs(p1|p2, k+1)
		}

		return c
	}

	out.Println(dfs(0, 0))
}

func main() {
	in, out := io.InitIo(&io.InitIoProps{
		DoBuffer: true,
		ReadLine: false,
	})
	defer out.Flush()

	calc(in, out)
}

