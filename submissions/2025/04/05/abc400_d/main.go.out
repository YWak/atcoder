package main

import (
	"fmt"
	gomath "math"
	"math/bits"
	"os"
	"strings"

	"github.com/ywak/atcoder/lib/ds"
	io "github.com/ywak/atcoder/lib/io"
	"github.com/ywak/atcoder/lib/math"
)

func calc(in *io.In, out *io.Out) {
	h, w := in.NextInt2()
	s := make([][]byte, h)
	for i := 0; i < h; i++ {
		s[i] = in.NextBytes()
	}

	a, b, c, d := in.NextInt4()
	a--
	b--
	c--
	d--

	isOut := func(x, y int) bool {
		return x < 0 || x >= h || y < 0 || y >= w
	}

	type item struct{ x, y, c int }
	queue := ds.NewPriorityQueue(func(a, b *item) bool { return a.c < b.c })
	costs := ds.NewInt2d(h, w, INF18)

	queue.Push(&item{a, b, 0})
	costs[a][b] = 0

	dx1 := []int{-0, -1, +0, +1}
	dy1 := []int{-1, -0, +1, +0}
	dx2 := []int{-0, -1, +0, +1, -0, -2, +0, +2}
	dy2 := []int{-1, -0, +1, +0, -2, -0, +2, +0}

	for queue.HasElements() {
		p := queue.Pop()
		if p.x == c && p.y == d {
			out.Println(p.c)
			return
		}

		for t := 0; t < 4; t++ {
			x, y := p.x+dx1[t], p.y+dy1[t]
			nx := p.c
			if isOut(x, y) || s[x][y] == '#' || costs[x][y] <= nx {
				continue
			}
			queue.Push(&item{x, y, nx})
			costs[x][y] = nx
		}
		// 前蹴り
		for t := 0; t < 8; t++ {
			x, y := p.x+dx2[t], p.y+dy2[t]
			nx := p.c + 1
			if isOut(x, y) || costs[x][y] <= nx {
				continue
			}
			queue.Push(&item{x, y, nx})
			costs[x][y] = nx
		}
	}
}

func main() {
	in, out := io.InitIo(&io.InitIoProps{
		DoBuffer: true, // インタラクティブならfalseにすること。
		ReadLine: false,
	})
	defer out.Flush()

	calc(in, out)
}

