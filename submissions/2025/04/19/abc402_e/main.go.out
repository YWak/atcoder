package main

import (
	"fmt"
	gomath "math"
	"math/bits"
	"os"
	"strings"

	io "github.com/ywak/atcoder/lib/io"
	"github.com/ywak/atcoder/lib/math"
)

func calc(in *io.In, out *io.Out) {
	n, X := in.NextInt2()
	s := make([]int, n)
	c := make([]int, n)
	p := make([]int, n)

	for i := 0; i < n; i++ {
		s[i] = in.NextInt()
		c[i] = in.NextInt()
		p[i] = in.NextInt()
	}

	// dp[x][p]はpの回答状況でx円もっているときの得点の期待値
	dp := make([][]float64, X+1)
	for i := range dp {
		dp[i] = make([]float64, 1<<n)

		for j := range dp[i] {
			dp[i][j] = -1
		}
	}

	// 全パターンを試す
	chmaxf := func(a *float64, b float64) {
		if *a < b {
			*a = b
		}
	}

	var dfs func(pat, x int) float64
	dfs = func(pat int, x int) float64 {
		if dp[x][pat] == -1 {
			ans := 0.0
			// 回答されていない問題のすべてのパターンを試す
			for i := 0; i < n; i++ {
				cost := x + c[i]
				if cost > X {
					continue
				}
				if nthbit(pat, i) == 0 {
					continue
				}

				ok := float64(p[i]) / 100.0
				ng := 1.0 - ok

				a := 0.0
				a += (dfs(pat-(1<<i), cost) + float64(s[i])) * ok
				a += dfs(pat, cost) * ng

				chmaxf(&ans, a)
			}
			dp[x][pat] = ans
		}

		return dp[x][pat]
	}

	ans := 0.0
	for pat := 0; pat < (1 << n); pat++ {
		chmaxf(&ans, dfs(pat, 0))
	}

	out.Printf("%.10f\n", ans)
}

func main() {
	in, out := io.InitIo(&io.InitIoProps{
		DoBuffer: true, // インタラクティブならfalseにすること。
		ReadLine: false,
	})
	defer out.Flush()

	calc(in, out)
}

