            "func (pq *pqueue) Push(x interface{}) {",
            "    n := len(*pq)",
            "    i := x.(*item)",
            "    i.index = n",
            "    *pq = append(*pq, x.(*item))",
            "}",
            "",
            "// キューからの取り出し",
            "func (pq *pqueue) Pop() interface{} {",
            "    old := *pq",
            "    n := len(old)",
            "    if n == 0 {",
            "        fmt.Println(\"queue is empty\")",
            "        return nil",
            "    } ",
            "    i := old[n-1]",
            "    i.index = -1",
            "    *pq = old[0:n-1]",
            "    return i",
            "}",
